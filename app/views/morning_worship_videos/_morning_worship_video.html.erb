<div id="<%= dom_id morning_worship_video %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Video:</strong>
    <%= morning_worship_video.video_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Speaker first name:</strong>
    <%= morning_worship_video.speaker_first_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Speaker last name:</strong>
    <%= morning_worship_video.speaker_last_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Theme:</strong>
    <%= morning_worship_video.theme %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Theme scripture:</strong>
    <%= morning_worship_video.theme_scripture %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Theme scripture text:</strong>
    <%= morning_worship_video.theme_scripture_text %>
  </div>
</div>
