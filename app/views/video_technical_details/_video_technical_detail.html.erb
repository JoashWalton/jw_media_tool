<div id="<%= dom_id video_technical_detail %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Video:</strong>
    <%= video_technical_detail.video_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Duration:</strong>
    <%= video_technical_detail.duration %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Bitrate:</strong>
    <%= video_technical_detail.bitrate %>
  </div>
  <div>
    <strong class="block font-medium mb-1">File size:</strong>
    <%= video_technical_detail.file_size %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Video stream:</strong>
    <%= video_technical_detail.video_stream %>
